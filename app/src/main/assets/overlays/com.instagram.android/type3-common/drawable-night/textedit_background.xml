<?xml version="1.0" encoding="utf-8"?>
<selector android:enterFadeDuration="150" android:exitFadeDuration="150"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@*android:color/background_floating_material_dark" />
            <stroke android:width="1.0dip" android:color="@*android:color/secondary_text_default_material_dark" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@*android:color/background_floating_material_dark" />
            <stroke android:width="1.0dip" android:color="@*android:color/white" />
            <corners android:radius="8.0dip" />
        </shape>
    </item>
</selector>
