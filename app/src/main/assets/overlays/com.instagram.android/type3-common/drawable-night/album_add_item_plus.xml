<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="line">
            <stroke android:width="2.0dip" android:color="@*android:color/white" />
        </shape>
    </item>
    <item>
        <rotate android:fromDegrees="90.0">
            <shape android:shape="line">
                <stroke android:width="2.0dip" android:color="@*android:color/white" />
            </shape>
        </rotate>
    </item>
</layer-list>
