<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape android:shape="line">
            <stroke android:width="2.0dip" android:color="@*android:color/secondary_text_default_material_dark" />
        </shape>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape android:shape="line">
                <stroke android:width="2.0dip" android:color="@*android:color/white" />
            </shape>
        </clip>
    </item>
</layer-list>
